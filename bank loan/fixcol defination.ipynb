   "source": [
    "def fix_col(df):\n",
    "    l = list(df.columns)\n",
    "    l1 = []\n",
    "    for i in l:\n",
    "        a= i.lower().replace(' ','_')\n",
    "        a = a.replace('-','_')\n",
    "        l1.append(a)\n",
    "    d = dict(zip(l,l1))\n",
    "    df.rename(d, axis=1, inplace=True)\n",
    "    return df"
   ]
